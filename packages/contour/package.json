{
  "name": "@kubernetes-models/contour",
  "version": "1.5.4",
  "description": "Contour models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/contour",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:v1beta1": "crd-generate --input https://raw.githubusercontent.com/projectcontour/contour/v1.5.0/examples/contour/01-crds.yaml --output ./gen",
    "build:v1": "crd-generate --input https://raw.githubusercontent.com/projectcontour/contour/v1.9.0/examples/contour/01-crds.yaml --output ./gen",
    "postbuild": "export-map generate --cwd gen --export gen/export-map.json",
    "prepack": "cp package.json README.md dist/ && export-map inject --package dist/package.json --export gen/export-map.json",
    "clean": "rimraf gen"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "contour"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kubernetes-models/base": "workspace:*",
    "@kubernetes-models/validate": "workspace:*",
    "kubernetes-models": "workspace:*",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@kubernetes-models/crd-generate": "workspace:*",
    "@kubernetes-models/export-map": "workspace:*",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
